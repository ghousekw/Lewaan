# Files to ignore during Railway deployment

# Environment files (Railway provides these)
.env
.env.*
!.env.example

# Development files
.git
.gitignore
.gitattributes
.editorconfig

# IDE
.idea
.vscode
*.swp
*.swo

# Documentation
/docs
README.md
CHANGELOG.md

# Tests
/tests
phpunit.xml
.phpunit.result.cache

# Development dependencies (installed during build)
/vendor
/node_modules

# Build artifacts (generated during deployment)
/public/hot
/public/storage
/storage/*.key
/bootstrap/cache/*

# Logs
*.log
storage/logs/*
