{
  "name": "lewan-cms-api",
  "version": "1.0.0",
  "description": "Lewan Interior Design CMS API",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "prisma:generate": "powershell -ExecutionPolicy Bypass -File prisma-env.ps1 && prisma generate",
    "prisma:migrate": "powershell -ExecutionPolicy Bypass -File prisma-env.ps1 && prisma migrate deploy",
    "prisma:studio": "powershell -ExecutionPolicy Bypass -File prisma-env.ps1 && prisma studio",
    "prisma:seed": "powershell -ExecutionPolicy Bypass -File prisma-env.ps1 && ts-node prisma/seed.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@prisma/client": "^7.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^2.0.1",
    "dotenv": "^17.2.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "prisma": "^7.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
